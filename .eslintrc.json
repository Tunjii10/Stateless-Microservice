{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": "off",
        "dot-notation": "off",
        "object-shorthand": "off",
        "max-len": "off" 
    }
}
